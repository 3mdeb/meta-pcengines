part /boot  --source bootimg-grub-legacy  --ondisk sda --label msdos --active --align 1024
part /      --source rootfs --rootfs-dir=rootfs-main  --ondisk sda --fstype=ext4 --label rootfsA --align 1024 --fixed-size 2048 --exclude-path data/
part        --source rootfs --rootfs-dir=rootfs-main  --ondisk sda --fstype=ext4 --label rootfsB --align 1024 --fixed-size 2048 --exclude-path data/
part /data  --source rootfs --rootfs-dir=rootfs-data  --ondisk sda --fstype=ext4 --label data    --align 1024 --fixed-size 10240
bootloader  --ptable msdos --timeout=5 --append="rootfstype=ext4 console=ttyS0,115200 earlyprintk=serial,ttyS0,115200"
